{
    "styleSheets": [
        "theme.css"
    ],
    "configOverrides": {
        "progressIndicator": {
            "inlineMode": true,
            "showTotal": true,
            "parentContainer": "progressIndicator",
            "side": "left",
            "visible": true,
            "showHandler": false,
            "hideHandlerOnMouseOut": true,
            "showRemainingProgress": true
        },
        
        "videoContainer": {
            "overPlaybackBar": false,
            "dynamicLayout" : {
                "portraitHorizontalAlignment": "align-center",
                "landscapeVerticalAlignment": "align-center"
            }
        },
        "plugins": {
            "es.upv.paella.customTimeProgressIndicator": { "enabled": false },
            "es.upv.paella.slideMapProgressBarPlugin": {"enabled": false },
            "es.upv.paella.volumeButtonPlugin": {
                "volumeAlwaysVisible": false
            },

            "es.upv.paella.dualVideoDynamic": {
                "tabIndexStart": 40,
                "allowSwitchSide": true
            },

            "es.upv.paella.dualVideoPiP": {
                "enabled": true,
                "validContent": [
                    { "id": "presenter-presentation-pip", "content": ["presenter","presentation"], "icon": "present-mode-3.svg", "title": "Picture-in-picture" },
                    { "id": "presenter-2-presentation-pip", "content": ["presenter-2","presentation"], "icon": "present-mode-3.svg", "title": "Picture-in-picture" },
                    { "id": "presenter-presenter-2-pip", "content": ["presenter","presenter-2"], "icon": "present-mode-3.svg", "title": "Picture-in-picture" }
                ],
                "dualVideoContentIds": [
                    "presenter-presentation-dynamic",
                    "presenter-2-presentation-dynamic",
                    "presenter-presenter-2-dynamic",
                    "presenter-presentation",
                    "presenter-2-presentation",
                    "presenter-presenter-2"
                ],
                "tabIndexStart": 40
            }
        }
    },
    "icons": [
        { "plugin": "es.upv.paella.playPauseButton", "identifier": "play", "icon": "./icons/play.svg" },
        { "plugin": "es.upv.paella.playPauseButton", "identifier": "pause", "icon": "./icons/pause.svg" },
        { "plugin": "es.upv.paella.playPauseButton", "identifier": "replay", "icon": "./icons/replay.svg" },

        { "plugin": "es.upv.paella.backwardButtonPlugin","identifier": "backwardIcon", "icon": "./icons/back-30-s.svg"},
        { "plugin": "es.upv.paella.forwardButtonPlugin","identifier": "forwardIcon", "icon": "./icons/forward-30-s.svg"},

        { "plugin": "es.upv.paella.volumeButtonPlugin","identifier": "volumeHighIcon", "icon": "./icons/volume-high.svg"},
        { "plugin": "es.upv.paella.volumeButtonPlugin","identifier": "volumeMidIcon", "icon": "./icons/volume-mid.svg"},
        { "plugin": "es.upv.paella.volumeButtonPlugin","identifier": "volumeLowIcon", "icon": "./icons/volume-low.svg"},
        { "plugin": "es.upv.paella.volumeButtonPlugin","identifier": "volumeMuteIcon", "icon": "./icons/volume-mute.svg"},

        { "plugin": "es.upv.paella.layoutSelector","identifier": "layoutIcon", "icon": "./icons/presentation-mode.svg"},

        { "plugin": "es.upv.paella.frameControlButtonPlugin","identifier": "photoIcon", "icon": "./icons/photo.svg"},

        { "plugin": "es.upv.paella.captionsSelectorPlugin", "identifier": "captionsIcon", "icon": "./icons/closed-captions.svg" },

        { "plugin": "es.upv.paella.fullscreenButton", "identifier": "fullscreenIcon", "icon": "./icons/fullscreen.svg" },
        { "plugin": "es.upv.paella.fullscreenButton", "identifier": "windowedIcon", "icon": "./icons/windowed.svg" }

    ]
}