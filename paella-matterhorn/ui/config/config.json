            "es.upv.paella.volumeRangePlugin": { "enabled": true, "showMasterVolume": true, "showSlaveVolume": false },
            "es.upv.paella.themeChooserPlugin": { "enabled": false }

			"edu.harvard.dce.bannerPlugin": {"enabled": false, "defaultValue":false},
