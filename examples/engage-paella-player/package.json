{
  "name": "engage-paella-player",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:ghpages": "USE_OC_SERVER_FROM_URL=true OC_PAELLA8_BASE_URL='/paella-opencast' OC_PRESENTATION_URL='https://develop.opencast.org' CONFIG_FOLDER='https://polimediaupv.github.io/paella-opencast/config/' OC_PAELLA8_URL_TEMPLATE='/paella-opencast/watch.html?id={videoId}&ocServer={OC_PRESENTATION_URL}' vite build",
    "build": "tsc && vite build && vite build -c vite.config.embedapi.ts --emptyOutDir false",
    "preview": "vite preview",
    "clean:build": "rimraf .turbo dist",
    "clean:deps": "rimraf node_modules"
  },
  "devDependencies": {
    "@asicupv/paella-opencast-component": "^2.0.1",
    "@tailwindcss/vite": "^4.1.11",
    "express": "^5.1.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.3",
    "vite": "^7.0.1"
  },
  "dependencies": {
    "@asicupv/paella-basic-plugins": "^2.5.0",
    "@asicupv/paella-embedapi": "^2.11.1",
    "@asicupv/paella-extra-plugins": "^2.7.0",
    "@asicupv/paella-opencast-core": "^2.0.1",
    "@asicupv/paella-opencast-plugins": "^2.0.1",
    "@asicupv/paella-slide-plugins": "^2.5.0",
    "@asicupv/paella-video-plugins": "^2.3.0",
    "@asicupv/paella-webgl-plugins": "^2.3.0",
    "@asicupv/paella-zoom-plugin": "^2.4.0",
    "vite-plugin-static-copy": "^3.1.4"
  }
}
